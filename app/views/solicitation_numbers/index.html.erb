<h1>Listing solicitation_numbers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solicitation_numbers.each do |solicitation_number| %>
      <tr>
        <td><%= solicitation_number.name %></td>
        <td><%= link_to 'Show', solicitation_number %></td>
        <td><%= link_to 'Edit', edit_solicitation_number_path(solicitation_number) %></td>
        <td><%= link_to 'Destroy', solicitation_number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Solicitation number', new_solicitation_number_path %>
